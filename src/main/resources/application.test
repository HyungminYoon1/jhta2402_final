spring:
  devtools:
    livereload:
      enabled: true
  application:
    name: home
  datasource:
    driver-class-name: org.postgresql.Driver
    url: jdbc:postgresql://localhost:[dp-port]/[dbname]
    username: [db-user]
    password: [db-pwd]
  jpa:
    database-platform: org.hibernate.dialect.PostgreSQLDialect
    database: postgresql
    hibernate:
      ddl-auto: update
server:
  port: [port]